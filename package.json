{
  "name": "@jmischler72/core-tetris",
  "version": "1.1.6",
  "description": "core game package for tetris-project",
  "scripts": {
    "test": "jest",
    "build": "rimraf dist && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && npm run copy",
    "copy": "run-script-os",
    "copy:darwin:linux": "cp ./package-cjs.json dist/cjs/package.json && cp ./package-mjs.json dist/mjs/package.json",
    "copy:win32": "xcopy \".\\package-cjs.json\" \"dist\\cjs\\package.json*\" && xcopy \".\\package-mjs.json\" \"dist\\mjs\\package.json*\"",
    "updateLink": "npm run build & npm link",
    "prepare": "npm run build"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "dist/mjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "author": "jmischler72",
  "license": "ISC",
  "repository": "https://github.com/jmischler72/core_tetris",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/jmischler72"
  },
  "devDependencies": {
    "@types/jest": "^29.5.7",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "run-script-os": "^1.1.6"
  },
  "files": [
    "dist/"
  ]
}
