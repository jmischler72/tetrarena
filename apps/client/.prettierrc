{
	"useTabs": true,
	"singleQuote": true,
	"printWidth":80,
	"semi":false,
	"bracketSameLine":true,
	"trailingComma":"es5",
	"tabWidth":2,
	"bracketSpacing":false,
	"arrowParens":"always",
	"singleAttributePerLine":true,
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
