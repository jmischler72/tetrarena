{
	"version": "1.1.10",
	"packages": ["packages/*", "apps/*"],
	"npmClient": "pnpm",
	"ignoreChanges": ["**/node_modules/**", "**/package.json", "**/*.md", ".github/**", "lerna.json", "pnpm-lock.yaml"],
	"command": {
		"version": {
			"conventionalCommits": true,
			"noGitTagVersion": true,
			"message": "chore(release): version %s"
		}
	}
}
